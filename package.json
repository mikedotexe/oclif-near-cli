{
  "name": "near",
  "description": "Interact with NEAR Protocol, a blockchain great for building smart contracts and so much more.",
  "version": "0.0.0",
  "author": "Mike Purvis @mikedotexe",
  "bin": {
    "near": "./bin/run"
  },
  "bugs": "https://github.com/mikedotexe/near-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.3",
    "@types/inquirer": "^6.5.0",
    "dotenv": "^8.2.0",
    "envfile": "^4.5.0",
    "inquirer": "^7.0.4",
    "nearlib": "^0.21.0",
    "query-string": "^6.10.1",
    "shelljs": "^0.8.5",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.8",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.14",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mikedotexe/near-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "macos": {
      "identifier": "com.nearprotocol.devcli"
    },
    "commands": "./lib/commands",
    "bin": "near-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "mikedotexe/near-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
